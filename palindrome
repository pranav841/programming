class pallindrome{
    public static void main(String args[]){
        String s = "boob";
        String rev="";
        
        for(int i = s.length()-1; i>=0; i-- )
        
        rev = rev + s.charAt(i);
        
        if (s.equals(rev)){
            System.out.println("pallindrome no");

        }
        else{
            System.out.println("non pallindrome");
        }



    }
}
